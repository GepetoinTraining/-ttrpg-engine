{
  "buildCommand": "bun run build",
  "outputDirectory": "fend/dist",
  "installCommand": "bun install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api/trpc/[trpc]"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
